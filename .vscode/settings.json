{
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "/Library/TeX/texbin/latexmk",
            "args": ["-r", "%DIR%/.latexmkrc", "-cd", "-g", "%DOC%"],
            "env": {}
        },
        {
            "name": "pdflatex",
            "command": "/Library/TeX/texbin/pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "env": {}
        }
    ],
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk (with bibtex)",
            "tools": ["latexmk"]
        },
        {
            "name": "pdflatex",
            "tools": ["pdflatex"]
        }
    ],
    "latex-workshop.latex.autoBuild.interval": 1000,
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.view.pdf.internal.synctex.keybinding": "double-click",
    "latex-workshop.latex.autoClean.run": "onBuilt",
    "latex-workshop.latex.outDir": "%DIR%",
    "latex-workshop.message.log.show": false,
    "latex-workshop.message.information.show": false,
    "latex-workshop.showContextMenu": true,
    "latex-workshop.formatting.latex": "latexindent",
    "latex-workshop.formatting.latexindent.args": [
        "-c",
        "%DIR%/",
        "%TMPFILE%",
        "-l",
        "/Users/kanegaeryota/dev/latex-template/latexindent.yaml",
        "-r"
    ],
    "latex-workshop.formatting.latexindent.path": "/opt/homebrew/bin/latexindent",
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.ist",
        "*.fls",
        "*.log",
        "*.fdb_latexmk"
    ],
    "files.autoSave": "onFocusChange",
    "files.exclude": {
        "**/__latexindent_temp.tex": true,
        "**/__pycache__": true,
        "**/_markdown_*/": true,
        "**/_minted-*/": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.svn": true,
        // "**/*_template.pdf": true,
        // "**/*.log": true
        "**/*.bbl": true,
        "**/*.bcf": true,
        "**/*.blg": true,
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.idx": true,
        "**/*.ind": true,
        "**/*.lof": true,
        "**/*.lot": true,
        "**/*.dvi": true,
        "**/*-blx.bib": true,
        "**/*.7z": true,
        "**/*.asv": true,
        "**/*.atfi": true,
        "**/*.aux": true,
        "**/*.gz": true,
        "**/*.markdown.in": true,
        "**/*.markdown.lua": true,
        "**/*.out": true,
        "**/*.pyg": true,
        "**/*.pytxcode": true,
        "**/*.rar": true,
        "**/*.run.xml": true,
        "**/*.stderr": true,
        "**/*.stdout": true,
        "**/*.tar": true,
        "**/*.tar.gz": true,
        "**/*.texmfcache": true,
        "**/*.tgz": true,
        "**/CVS": true,
        "**/pythontex-files-*/": true,
        "**/svg-inkscape*": true,
        "**/Thumbs.db": true
    },
    "files.associations": {
        "*.tex": "latex"
    },
    "latex-workshop.latex.recipe.default": "latexmk (with bibtex)",
    "latex-workshop.latex.build.forceRecipeUsage": false,
    "latex-workshop.latex.autoBuild.run": "onSave",
    "latex-workshop.latex.autoBuild.delay": 1000,
    "latex-workshop.latex.watch.files.ignore": ["**/_preamble.tex"],
    "latex-workshop.latex.rootFile.doNotPrompt": true,
    "latex-workshop.latex.rootFile.useSubFile": false,
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.view.pdf.zoom": "page-width",
    "[bibtex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop"
    }
}
